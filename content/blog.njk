---js
const eleventyNavigation = {
	key: "Archive",
	order: 2
};
---

<h1>Blog</h1>

{% set unreadPosts = collections.posts | rejectattr("fileSlug", "in", read.read) %}

<ul>
  {% for post in unreadPosts %}
    <li>
      <a href="{{ post.url }}">{{ post.data.title }}</a>
    </li>
  {% endfor %}
</ul>